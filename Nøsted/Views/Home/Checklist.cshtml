<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sjekkliste</title>
    <link rel="stylesheet" href="sjekkliste.css">
</head>
<body>
    @{
    ViewData["Title"] = "Checklist";
    }
    <h1>@ViewData["Title"]</h1>

    <form id="checklist" asp-action=""> <!-- må legge til en asp-action!! -->
        <div class="rowType" id="header">
            <div class="rowTypeHeader">
                <label for="serieInp">Serie nr.</label>
                <input id="serieInp" type="text">
            </div>
            <div>
                <label for="typeInp">Type:</label>
                <input id="typeInp" type="text">
            </div>
        </div>
        <div class="rowType" id="subHeader">
            <div></div>
            <div>Sjekkpunkter:</div>
            <div class="OBSD">Ok</div>
            <div class="OBSD">Bør skiftes</div>
            <div class="OBSD">Defekt</div>
        </div>
        <div class="rowType" id="mekanisk">
            <div class="rowTypeHeader">Mekanisk</div>
            <div id="mekaniskPunkter">
                <div class="sjekkpunkt">
                    <label id="sjekkpunkt1" for="radio1">Sjekk clutsj lameller for slitasje</label>
                    <div class="ok">
                        <input type="radio" id="radio1" name="sjekkpunkt1">
                    </div>
                    <div class="borSkiftes">
                        <input type="radio" id="radio1" name="sjekkpunkt1">
                    </div>
                    <div class="defekt">
                        <input type="radio" id="radio1" name="sjekkpunkt1">
                    </div>
                </div>
                <div class="sjekkpunkt">
                    <label id="sjekkpunkt2" for="radio2">Sjekk bremser. Bånd/Pal</label>
                    <div class="ok">
                        <input type="radio" id="radio2" name="sjekkpunkt2">
                    </div>
                    <div class="borSkiftes">
                        <input type="radio" id="radio2" name="sjekkpunkt2">
                    </div>
                    <div class="defekt">
                        <input type="radio" id="radio2" name="sjekkpunkt2">
                    </div>
                </div>
                <div class="sjekkpunkt">
                    <label id="sjekkpunkt3" for="radio3">Sjekk lager for trommel</label>
