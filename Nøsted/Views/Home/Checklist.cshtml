<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sjekkliste</title>
    <link rel="stylesheet" href="sjekkliste.css">
</head>
<body>
    @{
    ViewData["Title"] = "Checklist";
    }
    <h1>@ViewData["Title"]</h1>

    <form id="checklist" asp-action="Lag">
        <div class="rowType" id="header">
            <div class="rowTypeHeader">Serie nr.</div>
            <div>Type:</div>
        </div>
        <div class="rowType" id="subHeader">
            <div></div>
            <div>Sjekkpunkter:</div>
            <div>Ok</div>
            <div>Bør skiftes</div>
            <div>Defekt</div>
        </div>
        <div class="rowType" id="mekanisk">
            <div class="rowTypeHeader">Mekanisk</div>
            <div id="mekaniskPunkter">
                <div class="sjekkpunkt">
                    <div id="clutch" id="sjekkpunkt1">Sjekk clutch lameller for slitasje</div>
                    <div class="ok">
                        <input type="radio" name="sjekkpunkt1">
                    </div>
                    <div class="borSkiftes">
                        <input type="radio" name="sjekkpunkt1">
                    </div>
                    <div class="defekt">
                        <input type="radio" name="sjekkpunkt1">
                    </div>
                </div>
                <div class="sjekkpunkt">
                    <div id ="bremser" id="sjekkpunkt2">Sjekk bremser. Bånd/Pal</div>
                    <div class="ok">
                        <input type="radio" name="sjekkpunkt2">
                    </div>
                    <div class="borSkiftes">
                        <input type="radio" name="sjekkpunkt2">
                    </div>
                    <div class="defekt">
                        <input type="radio" name="sjekkpunkt2">
                    </div>
                </div>
                <div class="sjekkpunkt">
                    <div id ="lagerForTrommel" id="sjekkpunkt3">Sjekk lager for trommel</div>
                    <div class="ok">
                        <input type="radio" name="sjekkpunkt3">
                    </div>
                    <div class="borSkiftes">
                        <input type="radio" name="sjekkpunkt3">
                    </div>
                    <div class="defekt">
                        <input type="radio" name="sjekkpunkt3">
                    </div>
                </div>
                <div class="sjekkpunkt">
                    <div id ="pto" id="sjekkpunkt4">Sjekk PTO for opplagring</div>
                    <div class="ok">
                        <input type="radio" name="sjekkpunkt4">
                    </div>
                    <div class="borSkiftes">
                        <input type="radio" name="sjekkpunkt4">
                    </div>
                    <div class="defekt">
                        <input type="radio" name="sjekkpunkt4">
                    </div>
                </div>
                <div class="sjekkpunkt">
                    <div id ="kjedestrammer" id="sjekkpunkt5">Sjekk kjedestrammer</div>
                    <div class="ok">
                        <input type="radio" name="sjekkpunkt5">
                    </div>
                    <div class="borSkiftes">
                        <input type="radio" name="sjekkpunkt5">
                    </div>
                    <div class="defekt">
                        <input type="radio" name="sjekkpunkt5">
                    </div>
                </div>
                <div class="sjekkpunkt">
                    <!-- Spør Herman ! -->
                    <div id="sjekkpunkt6">Sjekk wire</div>
                    <div class="ok">
                        <input type="radio" name="sjekkpunkt6">
                    </div>
                    <div class="borSkiftes">
                        <input type="radio" name="sjekkpunkt6">
                    </div>
                    <div class="defekt">
                        <input type="radio" name="sjekkpunkt6">
                    </div>
                </div>
                <div class="sjekkpunkt">
                    <div id ="pinionLager" id="sjekkpunkt7">Sjekk pinion lager</div>
                    <div class="ok">
                        <input type="radio" name="sjekkpunkt7">
                    </div>
                    <div class="borSkiftes">
                        <input type="radio" name="sjekkpunkt7">
                    </div>
                    <div class="defekt">
                        <input type="radio" name="sjekkpunkt7">
                    </div>
                </div>
                <div class="sjekkpunkt">
