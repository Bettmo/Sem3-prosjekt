                    <div class="ok">
                        <input type="radio" id="radio6" name="sjekkpunkt6">
                    </div>
                    <div class="borSkiftes">
                        <input type="radio" id="radio6" name="sjekkpunkt6">
                    </div>
                    <div class="defekt">
                        <input type="radio" id="radio6" name="sjekkpunkt6">
                    </div>
                </div>
                <div class="sjekkpunkt">
