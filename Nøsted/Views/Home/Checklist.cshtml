                    <div class="ok">
                        <input type="radio" name="sjekkpunkt8">
                    </div>
                    <div class="borSkiftes">
                        <input type="radio" name="sjekkpunkt8">
                    </div>
                    <div class="defekt">
                        <input type="radio" name="sjekkpunkt8">
                    </div>
                </div>
                <div class="sjekkpunkt">
                    <div id="sjekkpunkt9">x</div>
                    <div class="ok">
                        <input type="radio" name="sjekkpunkt9">
                    </div>
                    <div class="borSkiftes">
                        <input type="radio" name="sjekkpunkt9">
                    </div>
                    <div class="defekt">
                        <input type="radio" name="sjekkpunkt9">
                    </div>
                </div>
            </div>
        </div>
        <div class="rowType" id="hydraulisk">
            <div class="rowTypeHeader">Hydraulisk</div>
            <div id="hydrauliskPunkter">
