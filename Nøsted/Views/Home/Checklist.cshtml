                        <input type="radio" id="radio8" name="sjekkpunkt8">
                    </div>
                    <div class="borSkiftes">
                        <input type="radio" id="radio8" name="sjekkpunkt8">
                    </div>
                    <div class="defekt">
                        <input type="radio" id="radio8" name="sjekkpunkt8">
                    </div>
                </div>
                <div class="sjekkpunkt">
                    <label id="sjekkpunkt9" for="radio9">x</label>
