                        <input type="radio" id="radio3" name="sjekkpunkt3">
                    </div>
                    <div class="borSkiftes">
                        <input type="radio" id="radio3" name="sjekkpunkt3">
                    </div>
                    <div class="defekt">
                        <input type="radio" id="radio3" name="sjekkpunkt3">
                    </div>
                </div>
                <div class="sjekkpunkt">
                    <label id="sjekkpunkt4" for="radio4">Sjekk PTO for opplagring</label>
                    <div class="ok">
                        <input type="radio" id="radio4" name="sjekkpunkt4">
                    </div>
                    <div class="borSkiftes">
                        <input type="radio" id="radio4" name="sjekkpunkt4">
                    </div>
                    <div class="defekt">
                        <input type="radio" id="radio4" name="sjekkpunkt4">
                    </div>
                </div>
                <div class="sjekkpunkt">
                    <label id="sjekkpunkt5" for="radio5">Sjekk kjedestrammer</label>
                    <div class="ok">
                        <input type="radio" id="radio5" name="sjekkpunkt5">
